{
	"name": "venne/widgets",
	"description": "Venne widgets component",
	"keywords": ["nette", "venne", "widgets"],
	"homepage": "http://venne.cz",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"support": {
		"email": "info@venne.cz",
		"issues": "https://github.com/Venne/widgets/issues"
	},
	"require": {
		"php": ">=5.5.0",
		"nette/application": "2.3@dev"
	},
	"require-dev": {
		"nette/tester": "~1.3.2",
		"phing/phing": "~2.10.1",
		"squizlabs/php_codesniffer": "~1.5.0"
	},
	"autoload": {
		"psr-4": {
			"Venne\\Widgets\\": "src/Widgets/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VenneTests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		},
		"venne": {
			"configuration": {
				"extensions": {
					"widgets": "Venne\\Widgets\\DI\\WidgetsExtension"
				}
			}
		}
	}
}